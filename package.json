{
  "name": "osmforcities",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "db:seed": "tsx prisma/seed.ts",
    "test:playwright": "NODE_ENV=test playwright test",
    "test:playwright:ui": "NODE_ENV=test playwright test --ui",
    "test:db:setup": "TEST_DATABASE_URL=postgresql://postgres@localhost:5433/osmforcities-test pnpm prisma migrate reset --force --skip-seed && pnpm prisma migrate deploy",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.823.0",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.80.7",
    "@turf/bbox": "^7.2.0",
    "@types/geojson": "^7946.0.16",
    "@types/osmtogeojson": "^2.2.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.513.0",
    "maplibre-gl": "^5.6.0",
    "next": "15.3.3",
    "osmtogeojson": "3.0.0-beta.5",
    "prisma": "^6.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-map-gl": "^8.0.4",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-playwright": "^2.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
