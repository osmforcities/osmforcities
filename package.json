{
  "name": "osmforcities",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:test": "NODE_ENV=test next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "db:seed": "tsx prisma/seed.ts",
    "test:playwright": "NODE_ENV=test playwright test",
    "test:playwright:ui": "NODE_ENV=test playwright test --ui",
    "prepare": "husky",
    "i18n:check": "i18n-check --locales messages --source en",
    "i18n:check:fix": "i18n-check --locales messages --source en --fix",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset version && git add ."
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-ses": "^3.823.0",
    "@mdx-js/loader": "^3.1.0",
    "@next/env": "^15.3.5",
    "@next/mdx": "^15.5.2",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.80.7",
    "@turf/bbox": "^7.2.0",
    "@turf/centroid": "^7.2.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/geojson": "^7946.0.16",
    "@types/osmtogeojson": "^2.2.34",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html-to-text": "^9.0.5",
    "lucide-react": "^0.513.0",
    "maplibre-gl": "^5.6.0",
    "next": "15.3.3",
    "next-auth": "5.0.0-beta.29",
    "next-intl": "^4.3.4",
    "osmtogeojson": "3.0.0-beta.5",
    "react": "^19.0.0",
    "react-aria-components": "^1.12.1",
    "react-dom": "^19.0.0",
    "react-map-gl": "^8.0.4",
    "react-stately": "^3.41.0",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@eslint/eslintrc": "^3",
    "@lingual/i18n-check": "^0.8.4",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/postcss": "^4",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-playwright": "^2.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prisma": "^6.11.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "messages/*.json": [
      "i18n-check --locales messages --source en"
    ]
  }
}
