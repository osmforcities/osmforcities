{
  "name": "osmforcities-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "dotenv -e .env prisma generate",
    "prisma:migrate": "dotenv -e .env prisma migrate dev",
    "prisma:seed": "dotenv -e .env prisma db seed",
    "prisma:studio": "dotenv -e .env prisma studio",
    "postinstall": "node ../../setup-env.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/node": "^20.8.7",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "csv-parse": "^5.5.2",
    "date-fns": "2.16.1",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "lodash.pick": "^4.4.0",
    "next": "13.4.13",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "^5.7.7",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1"
  }
}
